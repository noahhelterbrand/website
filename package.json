{
  "name": "firstwebsitevite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "concurrently -n Prettier,esLint \"yarn prettier:check\" \"yarn eslint:check\"",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "eslint:check": "eslint src --cache --cache-strategy content",
    "eslint:fix": "eslint src --fix --cache --cache-strategy content"
  },
  "dependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "2.1.3",
    "concurrently": "^9.0.1",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.29.1",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  },
  "packageManager": "yarn@4.5.1"
}
